<% if @library.errors.any? %>
  <strong>Please correct the following errors:</strong>
  <ul>
    <% @library.errors.full_messages.each do |e| %>
      <li>
        <%= e %>
      </li>
    <% end %><%# each %>
  </ul>
<% end %><%# if %>
<%= form_with model: @library, local: true do |f| %>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.submit %>
<% end %>