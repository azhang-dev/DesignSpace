<%= form_with model: @projects, local: true do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <br>
    <%= f.label :description %>
    <%= f.text_field :description %> 
    <br>
    <%= f.label :image_url, 'Image URL' %>
    <%= f.text_field :image_url %> 
    <br>

    <%= f.submit %>

<% end %>