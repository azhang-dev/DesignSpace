<%= form_with model: @project, local: true, html: { multipart: true }  do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :location %>
    <%= f.country_select :location %>
    <br>
    <%= f.label :description %>
    <%= f.text_field :description %> 
    <br>
    <%= f.label :image %>
    <%= f.file_field :image, :multiple => false %> 
    <br>
    <%= f.label :images %>
    <%= f.file_field :images, :multiple => true %> 
    <br>

    <%= f.submit %>

<% end %>